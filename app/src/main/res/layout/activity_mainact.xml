<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FBF9F9"
    tools:context=".MainAct">


    <ImageView
        android:id="@+id/GrdntYellow"
        android:layout_width="309dp"
        android:layout_height="364dp"
        android:layout_marginEnd="196dp"
        android:layout_marginBottom="592dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/yippy" />

    <ImageView
        android:id="@+id/grdtTeel"
        android:layout_width="508dp"
        android:layout_height="705dp"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="368dp"
        android:accessibilityLiveRegion="none"
        android:adjustViewBounds="true"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/teel" />

    <ImageView
        android:id="@+id/grdtPink"
        android:layout_width="561dp"
        android:layout_height="610dp"
        android:layout_marginTop="460dp"
        android:layout_marginEnd="72dp"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pink" />

    <ImageView
        android:id="@+id/grdtPlue"
        android:layout_width="400dp"
        android:layout_height="372dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.536"
        app:srcCompat="@drawable/plue" />

    <ImageView
        android:id="@+id/otlDblue"
        android:layout_width="348dp"
        android:layout_height="433dp"
        android:layout_marginStart="204dp"
        android:layout_marginBottom="568dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/otl_des" />

    <ImageView
        android:id="@+id/otlDgreen"
        android:layout_width="387dp"
        android:layout_height="529dp"
        android:layout_marginStart="-290dp"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/otl_desg" />

    <ImageView
        android:id="@+id/dPeach"
        android:layout_width="318dp"
        android:layout_height="448dp"
        android:layout_marginTop="515dp"
        android:layout_marginEnd="-190dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/peach" />

    <ImageView
        android:id="@+id/btnsettings"
        android:layout_width="30dp"
        android:layout_height="29dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.953"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.02"
        app:srcCompat="@drawable/setting"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="224dp"
        android:layout_height="232dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.31"
        app:srcCompat="@drawable/_slogo" />

    <ImageView
        android:id="@+id/btnstart"
        android:layout_width="266dp"
        android:layout_height="54dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.598"
        app:srcCompat="@drawable/start"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/btnLodgbook"
        android:layout_width="264dp"
        android:layout_height="57dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.676"
        app:srcCompat="@drawable/btnlogbook" />

    <ImageView
        android:id="@+id/btnfavqoutes"
        android:layout_width="265dp"
        android:layout_height="46dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.747"
        app:srcCompat="@drawable/favquotes" />


</androidx.constraintlayout.widget.ConstraintLayout>