<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Widget.Theme.Application.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="Widget.Theme.Application.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>

    <style name="every_chall">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="quote_for_y">
        <item name="android:textSize">28sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="here_s_a_da">
        <item name="android:textSize">18sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="suggestion">
        <item name="android:textSize">33sp</item>
        <item name="android:textFontWeight" tools:targetApi="p">1000</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="consider_ta">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="min_walk">
        <item name="android:textSize">26sp</item>
        <item name="android:textFontWeight" tools:targetApi="p">1000</item>
        <item name="android:textColor">#000000</item>
    </style>




</resources>